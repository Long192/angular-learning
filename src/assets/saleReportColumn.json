{
  "columns": [
    {
      "header": "Mã số",
      "binding": "GroupCode",
      "width": 70,
      "rowSpan": 2
    },
    {
      "header": "Chỉ tiêu",
      "binding": "GroupName",
      "width": 195,
      "rowSpan": 2
    },
    {
      "header": "Dvt",
      "binding": "Unit",
      "width": 60,
      "rowSpan": 2,
      "dataType": 1
    },
    {
      "header": "Số lượng",
      "binding": "Quantity",
      "width": 70,
      "align": "right",
      "rowSpan": 2
    },
    {
      "binding": "Date",
      "format": "d",
      "align": "right",
      "width": 90,
      "rowSpan": 2,
      "date": true,
      "dataType": 4
    },
    {
      "binding": "CheckBox",
      "width": 70,
      "align": "center",
      "rowSpan": 2,
      "type": "checkbox"
    },
    {
      "header": "Doanh số (chưa thuế)",
      "binding": "Amount2",
      "aggregate": "Sum",
      "rowSpan": 2,
      "format": "n0",
      "dataType": 2
    },
    {
      "header": "Tiền thanh toán",
      "group": "price",
      "colSpan": 3
    },
    {
      "header": "Lãi gộp (Doanh số - tiền vốn)",
      "binding": "Profit",
      "aggregate": "Sum",
      "rowSpan": 2,
      "format": "n0",
      "dataType": 2
    },
    {
      "header": "Chiết khấu",
      "binding": "DiscountAmount",
      "aggregate": "Sum",
      "rowSpan": 2,
      "format": "n0",
      "dataType": 2
    },
    {
      "header": "Tổng cộng",
      "binding": "TotalAmount",
      "aggregate": "Sum",
      "parent": "price",
      "level": 1,
      "format": "n0",
      "dataType": 2
    },
    {
      "header": "Thu tiền ngay",
      "binding": "Amount111",
      "aggregate": "Sum",
      "parent": "price",
      "level": 1,
      "format": "n0",
      "dataType": 2
    },
    {
      "header": "Công nợ",
      "binding": "Amount131",
      "aggregate": "Sum",
      "parent": "price",
      "level": 1,
      "format": "n0",
      "dataType": 2
    }
  ],
  "collumnGroup": ["SubTotalName"],
  "rules": [
    {
      "key": "Amount131",
      "operation": "e",
      "compareValue": 0,
      "property": "innerText",
      "value": ""
    },
    {
      "key": "DiscountAmount",
      "operation": "e",
      "compareValue": 0,
      "property": "innerText",
      "value": ""
    },
    {
      "key": "Amount2",
      "operation": "ge",
      "compareValue": 1000000,
      "property": "style.background-color",
      "excelProperty": {
        "fill":  {
          "bgColor": { "argb": "FF0000" },
          "fgColor": { "argb": "FF0000" },
          "pattern": "solid",
          "type": "pattern"
        }
      },
      "value": "#ff0000"
    },
    {
      "key": "TotalAmount",
      "operation": "ge",
      "compareValue": 1000000,
      "property": "style.color",
      "excelProperty": {
        "font": {
          "color": {"argb": "FF0000"}
        }
      },
      "value": "#ff0000"
    }
  ],
  "style": {
    "common": {
      "key": "sale-list",
      "gridHeight": "500px",
      "alternateStep": 1,
      "fotterText": "Tổng cộng"
    },
    "dynamix": {
      "header": [
        { "reportProperty": "VerticalAlign", "value": "middle" },
        {
          "important": true,
          "gridProperty": "background-color",
          "reportProperty": "BackgroundColor",
          "value": "#FFF8DC"
        },
        { "important": true, "gridProperty": "font-weight", "reportProperty": "FontWeight", "value": "Bold" },
        { "important": true, "gridProperty": "text-align", "reportProperty": "TextAlign", "value": "center" },
        { "important": true, "gridProperty": "justify-content", "value": "center" }
      ],
      "body": [
        { "important": true, "gridProperty": "padding", "reportProperty": "Padding", "value": "4px 6px 3px 6px" }
      ],
      "cell": [
        { "important": true, "gridProperty": "word-wrap", "value": "break-word" },
        { "important": true, "gridProperty": "white-space", "value": " break-spaces" }
      ],
      "alt": [{ "reportProperty": "BackgroundColor", "gridProperty": "background-color", "value": "#f7f7f7" }],
      "group": [
        {
          "important": true,
          "gridProperty": "background-color",
          "reportProperty": "BackgroundColor",
          "value": "#f5deb3"
        },
        { "important": true, "gridProperty": "font-weight", "reportProperty": "FontWeight", "value": "Bold" }
      ],
      "fotter": [
        {
          "important": true,
          "gridProperty": "background-color",
          "reportProperty": "BackgroundColor",
          "value": "#FFF8DC"
        },
        { "important": true, "gridProperty": "text-warp", "value": "warp" },
        {
          "important": true,
          "gridProperty": "word-wrap",
          "value": "break-word"
        },
        { "important": true, "gridProperty": "white-space", "value": " break-spaces" }
      ]
    }
  },
  "excelStyle": {
    "header": {
      "alignment": {
        "horizontal": "center",
        "vertical": "middle",
        "wrapText": true
      },
      "border": {
        "bottom": { "style": "medium" },
        "top": { "style": "medium" },
        "left": { "style": "medium" },
        "right": { "style": "medium" }
      },
      "fill": {
        "bgColor": { "argb": "FFF8DC" },
        "fgColor": { "argb": "FFF8DC" },
        "pattern": "solid",
        "type": "pattern"
      },
      "font": {
        "bold": true,
        "family": 2,
        "size": 11,
        "name": "Arial"
      }
    },
    "body": {
      "alignment": {
        "vertical": "middle",
        "wrapText": true
      },
      "border": {
        "top": { "style": "medium" },
        "left": { "style": "medium" },
        "bottom": { "style": "medium" },
        "right": { "style": "medium" }
      },
      "font": {
        "size": 11,
        "family": 2,
        "name": "Arial"
      }
    },
    "alt": {
      "alignment": {
        "vertical": "middle",
        "wrapText": true
      },
      "border": {
        "top": { "style": "medium" },
        "left": { "style": "medium" },
        "bottom": { "style": "medium" },
        "right": { "style": "medium" }
      },
      "fill": {
        "bgColor": { "argb": "F7F7F7" },
        "fgColor": { "argb": "F7F7F7" },
        "pattern": "solid",
        "type": "pattern"
      },
      "font": {
        "name": "Arial",
        "size": 11,
        "family": 2
      }
    },
    "fotter": {
      "alignment": {
        "horizontal": "center",
        "vertical": "middle",
        "wrapText": true
      },
      "border": {
        "bottom": { "style": "medium" },
        "top": { "style": "medium" },
        "left": { "style": "medium" },
        "right": { "style": "medium" }
      },
      "fill": {
        "bgColor": { "argb": "FFF8DC" },
        "fgColor": { "argb": "FFF8DC" },
        "pattern": "solid",
        "type": "pattern"
      },
      "font": {
        "bold": true,
        "family": 2,
        "size": 11,
        "name": "Arial"
      }
    },
    "group": {
      "alignment": {
        "horizontal": "center",
        "vertical": "middle",
        "wrapText": true
      },
      "border": {
        "bottom": { "style": "medium" },
        "top": { "style": "medium" },
        "left": { "style": "medium" },
        "right": { "style": "medium" }
      },
      "fill": {
        "bgColor": { "argb": "F5DEB3" },
        "fgColor": { "argb": "F5DEB3" },
        "pattern": "solid",
        "type": "pattern"
      },
      "font": {
        "bold": true,
        "family": 2,
        "size": 11,
        "name": "Arial"
      }
    }
  }
}
